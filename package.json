{
  "name": "justus",
  "version": "0.2.0",
  "description": "A JavaScript validation library, with types!",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./dts-generator": {
      "require": {
        "types": "./dist/dts-generator.d.ts",
        "default": "./dist/dts-generator.cjs"
      },
      "import": {
        "types": "./dist/dts-generator.d.ts",
        "default": "./dist/dts-generator.mjs"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "plug",
    "coverage": "plug coverage",
    "dev": "plug coverage -w src -w test",
    "lint": "plug lint",
    "test": "plug test",
    "transpile": "plug transpile"
  },
  "author": "Juit Developers <developers@juit.com>",
  "license": "MIT",
  "devDependencies": {
    "@plugjs/build": "^0.2.4",
    "@plugjs/tsd": "^0.2.3",
    "@types/chai": "^4.3.4",
    "chai": "^4.3.7",
    "tsd": "^0.25.0",
    "typescript": "^4.9.5"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/juitnow/justus.git"
  },
  "keywords": [
    "validation",
    "typescript",
    "javascript"
  ],
  "bugs": {
    "url": "https://github.com/juitnow/justus/issues"
  },
  "homepage": "https://github.com/juitnow/justus#readme",
  "files": [
    "*.md",
    "dist/",
    "src/"
  ]
}
